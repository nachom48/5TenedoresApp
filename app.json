{
  "expo": {
    "name": "5-Tenedores",
    "slug": "5-Tenedores",
    "description":"This is like a Tripadvisor App but for Restaurants",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#00a680"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#00a680"
      },
      "package": "com.example.TenedoresApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "fc936915-9acb-4fb4-a6ab-6eacbeaedeb5"
      }
    },
    "plugins":[
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(5-Tenedores) to use your location."
        }
      ]
    ]
  }
}
